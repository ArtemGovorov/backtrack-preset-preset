{
  "name": "@backtrack/preset-preset",
  "version": "0.1.4",
  "description": "Backtrack preset to create Backtrack presets",
  "keywords": [
    "backtrack",
    "preset"
  ],
  "license": "ISC",
  "author": "Chris Blossom",
  "repository": "chrisblossom/backtrack-preset-preset",
  "homepage": "https://github.com/chrisblossom/backtrack-preset-preset",
  "engines": {
    "node": ">=6.9.0",
    "npm": ">=3.10.10",
    "yarn": ">=1.6.0"
  },
  "files": [
    "lib/"
  ],
  "main": "lib/preset-preset.js",
  "scripts": {
    "format": "backtrack format",
    "lint": "backtrack lint",
    "lint.fix": "backtrack lint.fix",
    "precommit": "lint-staged",
    "prepublishOnly": "npm run lint",
    "prepush": "npm run lint"
  },
  "lint-staged": {
    "*.{js,md,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "backtrack": {
    "presets": [
      "./"
    ]
  },
  "devDependencies": {
    "@backtrack/core": "^0.1.9"
  },
  "dependencies": {
    "@backtrack/preset-style": "^0.1.0",
    "eslint-plugin-node": "^6.0.1",
    "read-pkg-up": "^3.0.0"
  }
}
